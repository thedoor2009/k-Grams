<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <TempDir>$(SolutionDir)temp\$(PlatformName)\$(Configuration)\</TempDir>
    <IntermediateDir>$(TempDir)intermediates\$(ProjectName)\</IntermediateDir>
    <BinDir>$(TempDir)bin\</BinDir>
    <ScriptDir>$(SolutionDir)Scripts\</ScriptDir>
    <GameDir>$(TempDir)game\</GameDir>
    <AuthoredAssetDir>$(SolutionDir)Assets\</AuthoredAssetDir>
    <LibDir>$(TempDir)lib\</LibDir>
    <BuiltAssetDir>$(GameDir)data\</BuiltAssetDir>
    <EngineDir>$(TempDir)engine\</EngineDir>
    <ToolDir>$(SolutionDir)Code\Tools</ToolDir>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(GameDir)</OutDir>
  </PropertyGroup>
  <PropertyGroup>
    <IntDir>$(Configuration)\</IntDir>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="TempDir">
      <Value>$(TempDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="IntermediateDir">
      <Value>$(IntermediateDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="BinDir">
      <Value>$(BinDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="ScriptDir">
      <Value>$(ScriptDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="GameDir">
      <Value>$(GameDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="AuthoredAssetDir">
      <Value>$(AuthoredAssetDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="LibDir">
      <Value>$(LibDir)</Value>
    </BuildMacro>
    <BuildMacro Include="BuiltAssetDir">
      <Value>$(BuiltAssetDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="EngineDir">
      <Value>$(EngineDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="ToolDir">
      <Value>$(ToolDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>